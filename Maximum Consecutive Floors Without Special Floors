class Solution {
public:
    int maxConsecutive(int bottom, int top, vector<int>& special) {
        sort(special.begin(), special.end());
        int maxi = 0;
        int n = special.size();
        for (int i = 0; i < n; i++) {
            if (bottom != special[i])
                maxi = max(maxi, special[i] - bottom);

            bottom = special[i] + 1;
        }
        if (special[n - 1] != top)
            maxi = max(maxi, (top - bottom + 1));
        return maxi;
    }
};
