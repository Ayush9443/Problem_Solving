class Solution {
public:
    vector<int> findingUsersActiveMinutes(vector<vector<int>>& logs, int k) {
        vector<int> ans(k, 0);
        unordered_map<int, set<int>> v;
        for (auto& l : logs) {
            v[l[0]].insert(l[1]);
        }
        for (auto& p : v) {
            set<int> s = p.second;
            int size = s.size();
            ans[size - 1]++;
        }
        return ans;
    }
};
