class Solution {
public:
    vector<int> findMissingElements(vector<int>& nums) {
        int n = nums.size();
        vector<int> miss;
        sort(nums.begin(), nums.end());
        int j = 0;
        for (int i = nums[0]; i <= nums[n - 1]; i++) {
            if (i != nums[j]) {
                miss.push_back(i);
            } else {
                j++;
            }
        }
        return miss;
    }
};
